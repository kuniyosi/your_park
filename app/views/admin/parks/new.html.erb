<h1>投稿フォーム</h1>
  <%= form_with model: [:admin,@park] do |f| %>
    <h4>画像</h4>
      <%= f.file_field :image, accept: "image/*" %>
    <h4>公園名</h4>
      <%= f.text_field :name %>
    <h4>住所</h4>
      <%= f.text_field :address %>
    <h4>紹介</h4>
      <%= f.text_area :introduction %>
    <h4>エリア</h4>
      <%= f.text_field :area %>
    <%= f.submit '投稿' %>
  <% end %>